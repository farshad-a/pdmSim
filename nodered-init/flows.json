    {
        "id": "cdd3d0a8.04bb6",
        "type": "function",
        "z": "d907382e.777fa8",
        "name": "STORAGE STATION",
        "func": "let total_good_quality = msg.payload;\n\nvar storage = context.get('storage')||0;\n//required to calculate shared_sink\nvar total_dispatched = context.get('total_dispatched')||0;\n\n//vacuum pressure\nlet flow_in = Math.floor(Math.random() * (25 - 10)) + 10; \nstorage += flow_in;\n//vibration frequency\nlet flow_out = Math.floor(Math.random() * (10 - 5)) + 5 ;\n\n//prevent negative data for whole numbers range\nif (flow_out > storage)\n{\n    total_dispatched += storage;\n    flow_out = storage;\n    storage -= storage; //seting storage to zero\n}\nelse\n{\n    total_dispatched += flow_out;\n    storage -= flow_out;\n}\n\ncontext.set('total_dispatched',total_dispatched);\ncontext.set('storage', storage);\n\nvar shared_sink = context.get('shared_sink')||0;\nshared_sink = total_good_quality - (storage + total_dispatched);\ncontext.set('shared_sink',shared_sink);\n\nconst timestamp = new Date();\nmsg.topic = 'storage';\nmsg.payload = [shared_sink, storage, flow_in, flow_out, timestamp.toISOString()];\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 280,
        "y": 380,
