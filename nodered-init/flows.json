    {
        "id": "b04d9fa4.f56fa",
        "type": "function",
        "z": "d907382e.777fa8",
        "name": "QUALITY SORTING STATION",
        "func": "//initialise the counter to 0 if it doesn't exist already\nvar total_good_quality_corn = context.get('total_good_quality_corn')||0;\nvar total_bad_quality_corn = context.get('total_bad_quality_corn')||0;\n\ntotal_good_quality_corn += Math.floor(Math.random() * (40 - 25)) + 25;\ntotal_bad_quality_corn += Math.floor(Math.random() * (20 - 5)) + 5;\n\n// store the value back\ncontext.set('total_good_quality_corn',total_good_quality_corn);\ncontext.set('total_bad_quality_corn',total_bad_quality_corn);\n\nconst timestamp = new Date();\nmsg.topic = 'quality sorting';\nmsg.payload = [total_good_quality_corn, total_bad_quality_corn, timestamp.toISOString()];\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
